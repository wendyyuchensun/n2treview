class Move {
	method void move (int length) {
		var int i;
		let i = 1;
		while (i < length) {
			let snake[i - 1] = snake[i];
			let i = i + 1;
		}
		return;
	}
	method void moveRight (int length) {
		let snake[length - 1] = snake[length -1] + 1;
		return;
	}
	method void moveLeft (int length) {
		let snake[length - 1] = snake[length -1] - 1;
		return;
	}
}
